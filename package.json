{
    "name": "@vidbeo/player-api",
    "version": "1.0.0",
    "description": "Control a player embedded in an iframe",
    "author": "Vidbeo <https://www.vidbeo.com>",
    "repository": "vidbeo/player-api",
    "keywords": [
        "vidbeo",
        "player",
        "api",
        "iframe"
    ],
    "scripts": {
        "clean": "rimraf dist/*",
        "prebuild": "npm run clean",
        "format": "prettier 'src/**/*.js' 'test/**/*.js' --write",
        "test": "nyc --reporter=text ava",
        "build": "rollup --config",
        "build:local": "yalc push",
        "watch:build": "rollup --config --watch --environment STAGE:local",
        "prepublish:public": "npm run build && npm run bump-version",
        "publish:public": "npm publish --access public",
        "rimraf": "./node_modules/rimraf/bin.js"
    },
    "main": "dist/api.js",
    "module": "dist/api.es.js",
    "files": [
        "dist",
        "README"
    ],
    "ava": {
        "require": [
            "@babel/register",
            "@babel/polyfill",
            "./test/helpers/setup-browser-env.js"
        ],
        "babel": true,
        "inherit": true
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env"
            ]
        ]
    },
    "dependencies": {
        "loglevel": "^1.7.1"
    },
    "devDependencies": {
        "@ava/babel": "^2.0.0",
        "@babel/core": "^7.16.0",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.16.0",
        "@babel/register": "^7.16.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@rollup/plugin-replace": "^3.0.0",
        "ava": "^3.15.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-register": "^6.26.0",
        "browser-env": "^3.3.0",
        "eslint": "^7.7.0",
        "jsdom": "^18.0.1",
        "nyc": "^15.1.0",
        "prettier": "^2.4.1",
        "rimraf": "^3.0.2",
        "rollup": "^2.59.0",
        "rollup-plugin-terser": "^7.0.2",
        "sinon": "^12.0.0",
        "window": "^4.2.7"
    }
}
